{
  "storage": {
    "user": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "CardDetails": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "githublocAuthentication": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "user",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    },
    "gLOCcardDetails": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "CardDetails",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    }
  },
  "api": {
    "locapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "githublocAuthentication",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "gLOCcardDetails",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}